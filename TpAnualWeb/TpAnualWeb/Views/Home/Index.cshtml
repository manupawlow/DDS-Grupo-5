    <header class="bg-primary text-white">
        <div class="container text-center">
            <h1>GeSoc</h1>
            <p class="lead">Gestion de Proyectos Sociales</p>
        </div>
    </header>

    <!---------------------------------EGRESOS--------------------------------->

    <section id="about">
        <div class="container">
            <div class="row">
                <div class="col-lg-8 mx-auto">
                    <h2>EGRESOS</h2>

                    <p class="lead">Seleccione una opcion:</p>
                    <button onclick="myFunction(1)" target="_blank" class="button button2">Buscar Egreso</button>

                    <div id="1" style="display:none;">

                        <h4></h4>
                        @using (Html.BeginForm("MostrarEgresos", "Home", FormMethod.Post))
                        {
                            <input type="submit" value="Mostrar todos los egresos" class="button button4">
                        }
                        <p>Buscar egreso por id:</p>
                        @using (Html.BeginForm("BuscarEgreso", "Home", FormMethod.Post))
                        {
                            <input type="number" name="id_egreso" placeholder="ID del Egreso" class="button button3" />
                            <input type="submit" value="Buscar" class="button button4">
                        }
                        <p></p>

                    </div>

                    <h6></h6>
                    <button onclick="myFunction(2)" target="_blank" class="button button2">Cargar nuevo egreso</button>
                    <div id="2" style="display:none;">

                        <p><button id="addInput" class="button button3"> Agregar item </button></p>
                        @using (Html.BeginForm("CargarEgreso", "Home", FormMethod.Post))
                        {
                            <div id="itemsInput"></div>
                            <p><input type="text" name="descripcion" placeholder="Descripcion" class="button button3"></p>
                            <p><input type="text" name="revisor" placeholder="Usuario revisor" class="button button3"></p>
                            <p><input type="number" name="cantPresup" placeholder="Cantidad de presupuestos requeridos" class="button button3"></p>
                            <p><input type="submit" id="cargarEgreso" value="Cargar egreso" class="button button4"></p>
                        }

                    </div>

                    <h6></h6>
                    <button onclick="myFunction(3)" target="_blank" class="button button2">Validar Egreso</button>
                    <div id="3" style="display:none;">

                        <h4></h4>
                        @using (Html.BeginForm("ValidarEgreso", "Home", FormMethod.Post))
                        {
                            <input type="number" name="id_egreso" placeholder="ID del Egreso" class="button button3">
                            <p>Seleccionar criterio de validacion:</p>
                            <input type="submit" value="Menor valor" class="button button4">
                            <p></p>
                        }

                    </div>

                    <h6></h6>
                    <button onclick="myFunction(4)" target="_blank" class="button button2">Bandeja de Mensajes de Egreso</button>
                    <div id="4" style="display:none;">

                        @using (Html.BeginForm("BandejaDeMensajesDeEgreso", "Home", FormMethod.Post))
                        {
                            <p>
                                <input type="number" name="id_egreso" placeholder="ID del Egreso" class="button button3">
                                <input type="submit" value="Mostrar bandeja" class="button button4">
                            </p>
                        }

                    </div>

                </div>
            </div>
        </div>
    </section>

    <!---------------------------------INGRESOS--------------------------------->

    <section id="services" class="bg-light">
        <div class="container">
            <div class="row">
                <div class="col-lg-8 mx-auto">

                    <h2>INGRESOS</h2>
                    <p class="lead">Seleccione una opcion:</p>

                    <h6></h6>
                    <button onclick="myFunction(10)" target="_blank" class="button button2">Buscar Ingreso</button>

                    <div id="10" style="display:none;">

                        <h6></h6>
                        @using (Html.BeginForm("MostrarIngresos", "Home", FormMethod.Post))
                        {
                            <input type="submit" value="Mostrar todos los ingresos" class="button button4">
                        }
                        <p>Buscar ingreso por id:</p>
                        @using (Html.BeginForm("BuscarIngreso", "Home", FormMethod.Post))
                        {
                            <input type="text" name="id_ingreso" placeholder="ID del ingreso" class="button button3" />
                            <input type="submit" value="Buscar" class="button button4">
                        }
                        <p></p>

                    </div>

                    <h6></h6>
                    <button onclick="myFunction(11)" target="_blank" class="button button2">Cargar nuevo ingreso</button>
                    <div id="11" style="display:none;">

                        @using (Html.BeginForm("CargarIngreso", "Home", FormMethod.Post))
                        {
                            <p><input type="text" name="descripcion" placeholder="Descripcion" class="button button3"></p>
                            <p><input type="text" name="total" placeholder="Total" class="button button3"></p>

                            <p><button id="cargarIngreso" class="button button4">Cargar ingreso</button></p>

                            <div id="ingresoInfo"></div>
                        }
                    </div>

                    <h6></h6>
                    <button onclick="myFunction(12)" target="_blank" class="button button2">Asociar ingresos y egresos</button>

                    <div id="12" style="display:none;">
                        <p>Seleccione el proceso de vinculacion</p>
                        <!--Mirar con juan-->
                        @using (Html.BeginForm("FechaPrimerEgreso", "Home", FormMethod.Post))
                        {
                            <button id="fechaPrimerEgreso" class="button button4">Fecha primer egreso</button>
                        }
                        @using (Html.BeginForm("ValorPrimerEgreso", "Home", FormMethod.Post))
                        {
                            <button id="fechaPrimerEgreso" class="button button4">Valor primer egreso</button>
                        }
                        @using (Html.BeginForm("ValorPrimerIngreso", "Home", FormMethod.Post))
                        {
                            <button id="fechaPrimerEgreso" class="button button4">Valor primer ingreso</button>
                        }


                    </div>

                </div>
            </div>
        </div>
    </section>

    <!---------------------------------PROYECTOS--------------------------------->

    <section id="proyectos">
        <div class="container">
            <div class="row">
                <div class="col-lg-8 mx-auto">

                    <h2>PROYECTOS</h2>
                    <p class="lead">Seleccione una opcion:</p>


                    <h6></h6>
                    <button onclick="myFunction(30)" target="_blank" class="button button2">Buscar Proyectos</button>
                    <div id="30" style="display:none;">

                        <h4></h4>
                        @using (Html.BeginForm("MostrarProyectos", "Home", FormMethod.Post))
                        {
                            <input type="submit" value="Mostrar todos los proyectos" class="button button4">
                        }
                        <p>Buscar proyecto por id:</p>
                        @using (Html.BeginForm("BuscarProyecto", "Home", FormMethod.Post))
                        {
                            <input type="number" name="id_proyecto" placeholder="ID del Proyecto" class="button button3" />
                            <input type="submit" value="Buscar" class="button button4">
                        }
                        <p></p>

                    </div>



                    <p></p>
                    <button onclick="myFunction(31)" target="_blank" class="button button2">Cargar proyecto</button>
                    <div id="31" style="display:none;">
                        @using (Html.BeginForm("CargarProyecto", "Home", FormMethod.Post))
                        {
                            <input type="number" name="monto" placeholder="Monto" class="button button3">
                            <input type="number" name="cant_presupuestos" placeholder="Cantidad presupuestos minimos" class="button button3">
                            <input type="text" name="usuario" placeholder="Director de proyecto" class="button button3">
                            <input type="submit" value="Cargar" class="button button4">
                        }

                    </div>

                    <p></p>
                    <button onclick="myFunction(32)" target="_blank" class="button button2">Vincular ingreso a proyecto</button>
                    <div id="32" style="display:none;">
                        @using (Html.BeginForm("VincularIngresoConProyecto", "Home", FormMethod.Post))
                        {
                            <input type="number" name="id_ingreso" placeholder="ID Ingreso" class="button button3">
                            <input type="number" name="id_proyecto" placeholder="ID Proyecto" class="button button3">
                            <input type="submit" value="Vincular" class="button button4">
                        }

                    </div>


                    <p></p>
                    <button onclick="myFunction(33)" target="_blank" class="button button2">Vincular egreso a proyecto</button>
                    <div id="33" style="display:none;">
                        @using (Html.BeginForm("VincularEgresoConProyecto", "Home", FormMethod.Post))
                        {
                            <input type="number" name="id_egreso" placeholder="ID Egreso" class="button button3">
                            <input type="number" name="id_proyecto" placeholder="ID Proyecto" class="button button3">
                            <input type="submit" value="Vincular" class="button button4">
                        }

                    </div>


                    <p></p>
                    <button onclick="myFunction(34)" target="_blank" class="button button2">Dar de baja proyecto</button>
                    <div id="34" style="display:none;">
                        @using (Html.BeginForm("BajaProyecto", "Home", FormMethod.Post))
                        {
                            <input type="number" name="id_proyecto" placeholder="ID Proyecto" class="button button3">
                            <input type="submit" value="Borrar" class="button button4">
                        }

                    </div>

                </div>
            </div>
        </div>
    </section>

    <!---------------------------------PROVEEDORES--------------------------------->

    <section id="proveedores" class="bg-light">
        <div class="container">
            <div class="row">
                <div class="col-lg-8 mx-auto">

                    <h2>PROVEEDORES</h2>
                    <p class="lead">Seleccione una opcion:</p>

                    <h6></h6>
                    <button onclick="myFunction(40)" target="_blank" class="button button2">Buscar Proveedores</button>
                    <div id="40" style="display:none;">

                        <h4></h4>
                        @using (Html.BeginForm("MostrarProveedores", "Home", FormMethod.Post))
                        {
                            <input type="submit" value="Mostrar todos los proveedores" class="button button4">
                        }

                    </div>

                    <p></p>
                    <button onclick="myFunction(41)" target="_blank" class="button button2">Registrar Proveedor</button>
                    <div id="41" style="display:none;">

                        @using (Html.BeginForm("CargarProveedor", "Home", FormMethod.Post))
                        {
                            <p><input type="text" name="CUIT" placeholder="CUIT" class="button button3"></p>
                            <p><input type="text" name="razon" placeholder="Razon Social" class="button button3"></p>

                            <p><button id="registrarProveedor" class="button button4">Cargar proveedor</button></p>
                        }

                    </div>

                </div>
            </div>
        </div>
    </section>

    <!---------------------------------PRESUPUESTOS--------------------------------->

    <section id="presupuestos">
        <div class="container">
            <div class="row">
                <div class="col-lg-8 mx-auto">

                    <h2>PRESUPUESTOS</h2>
                    <p class="lead">Seleccione una opcion:</p>

                    <p></p>
                    <button onclick="myFunction(20)" target="_blank" class="button button2">Buscar presupuesto</button>
                    <div id="20" style="display:none;">
                        <h6></h6>
                        @using (Html.BeginForm("MostrarPresupuestos", "Home", FormMethod.Post))
                        {
                            <input type="submit" value="Mostrar todos los presupuestos" class="button button4">
                        }
                        <p>Buscar presupuesto por id:</p>
                        @using (Html.BeginForm("MostrarPresupuestoConItems", "Home", FormMethod.Post))
                        {
                            <input type="text" name="id_presupuesto" placeholder="ID del Presupuesto" class="button button3" />
                            <input type="submit" value="Buscar" class="button button4">
                        }
                        <p>Buscar presupuesto por egreso:</p>
                        @using (Html.BeginForm("MostrarPresupuestosPorEgreso", "Home", FormMethod.Post))
                        {
                            <input type="number" name="id_egreso" placeholder="ID del Egreso" class="button button3">
                            <input type="submit" value="Buscar" class="button button4">
                        }
                    </div>
                    <p></p>
                    <h6></h6>
                    <button onclick="myFunction(21)" target="_blank" class="button button2">Cargar nuevo presupuesto</button>
                    <div id="21" style="display:none;">
                        <p><button id="addInputPresupuesto" class="button button3"> Agregar item </button></p>
                        @using (Html.BeginForm("AgregarPresupuesto", "Home", FormMethod.Post))
                        {
                            <div id="itemsInputPresupuesto"></div>
                            <p><input type="number" name="id_egreso" placeholder="ID del Egreso" class="button button3"></p>
                            <p><input type="text" name="CUIT" placeholder="CUIT Proveedor" class="button button3"></p>
                            <p><input type="submit" id="agregarPresupuesto" value="Cargar presupuesto" class="button button4"></p>

                        }
                    </div>
                    <p></p>
                    <button onclick="myFunction(22)" target="_blank" class="button button2">Elegir presupuesto</button>
                    <div id="22" style="display:none;">
                        @using (Html.BeginForm("ElegirPresupuesto", "Home", FormMethod.Post))
                        {
                            <input type="number" name="id_egreso" placeholder="ID del Egreso" class="button button3">
                            <input type="number" name="id_presupuesto" placeholder="ID del Presupuesto" class="button button3">
                            <input type="submit" value="Elegir" class="button button4">
                        }

                    </div>

                </div>

            </div>
        </div>
    </section>

    <!---------------------------------CRITERIOS Y CATEGORIAS--------------------------------->
    <section id="criterios_y_categorias" class="bg-light">
        <div class="container">
            <div class="row">
                <div class="col-lg-8 mx-auto">

                    <h2>CRITERIOS Y CATEGORIAS</h2>
                    <p class="lead">Seleccione una opcion:</p>

                    <h6></h6>
                    <button onclick="myFunction(50)" target="_blank" class="button button2">Buscar Criterios</button>
                    <div id="50" style="display:none;">

                        <h4></h4>
                        @using (Html.BeginForm("MostrarCriterios", "Home", FormMethod.Post))
                        {
                            <input type="submit" value="Mostrar todos los criterios" class="button button4">
                        }

                    </div>

                    <h6></h6>
                    <button onclick="myFunction(51)" target="_blank" class="button button2">Buscar Categorias</button>
                    <div id="51" style="display:none;">

                        <h4></h4>
                        @using (Html.BeginForm("MostrarCategorias", "Home", FormMethod.Post))
                        {
                            <input type="submit" value="Mostrar todas las categorias" class="button button4">
                        }

                    </div>

                    <h6></h6>
                    <button onclick="myFunction(52)" target="_blank" class="button button2">Agregar criterio a item</button>
                    <div id="52" style="display:none;">

                        @using (Html.BeginForm("AsignarCriterio", "Home", FormMethod.Post))
                        {
                            <p><input type="text" name="item_desc" placeholder="Descripcion item" class="button button3"></p>
                            <p><input type="text" name="criterio" placeholder="Criterio" class="button button3"></p>
                            <p><input type="text" name="categoria" placeholder="Categoria" class="button button3"></p>

                            <p><button id="asignarCriterio" class="button button4">Asignar</button></p>
                        }

                    </div>
                    <h6></h6>

                    <button onclick="myFunction(53)" target="_blank" class="button button2">Cargar criterio</button>
                    <div id="53" style="display:none;">

                        @using (Html.BeginForm("CargarCriterio", "Home", FormMethod.Post))
                        {
                            <p><input type="text" name="descripcion" placeholder="Descripcion" class="button button3"></p>
                            <p><input type="number" name="jerarquia" placeholder="Jerarquia" class="button button3"></p>

                            <p><input type="submit" value="Cargar criterio" class="button button4"></p>
                        }
                    </div>
                    <h6></h6>

                    <button onclick="myFunction(54)" target="_blank" class="button button2">Cargar categoria a criterio</button>
                    <div id="54" style="display:none;">

                        @using (Html.BeginForm("CargarCategoriaAcriterio", "Home", FormMethod.Post))
                        {
                            <p><input type="text" name="descripcionCat" placeholder="Descripcion categoria" class="button button3"></p>
                            <p><input type="text" name="descripcionCrit" placeholder="Descripcion criterio" class="button button3"></p>
                            <p><input type="submit" value="Cargar categoria" class="button button4"></p>
                        }
                    </div>
                    <h6></h6>


                </div>
            </div>
        </div>
    </section>

    <!---------------------------------BITACORA DE OPERACIONES--------------------------------->
    <section id="bitacora_de_operaciones">
        <div class="container">
            <div class="row">

                <div class="col-lg-8 mx-auto">
                    <h2>BITACORA DE OPERACIONES</h2>
                    @using (Html.BeginForm("verBitacora", "Home", FormMethod.Post))
