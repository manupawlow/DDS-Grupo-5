@{
    ViewBag.Title = "Ingreso";
}



<!DOCTYPE html>
<html lang="en">
<meta name="viewport" content="width=device-width" />
<head>

    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta name="description" content="">
    <meta name="author" content="">

    <title>TP ANUAL DDS 2020 GRUPO 5</title>

    <!-- Bootstrap core CSS -->
    <link href="~/Content/boostrap/bootstrap.min.css" rel="stylesheet">
    <!-- Custom styles for this template -->
    <link href="~/Content/scrolling-nav.css" rel="stylesheet">

</head>

<!-- Bootstrap core JavaScript -->
<script src="~/Scripts/vendor/jquery/jquery.min.js"></script>
<script src="~/Scripts/vendor/bootstrap/js/bootstrap.bundle.min.js"></script>

<!-- Plugin JavaScript -->
<script src="~/Scripts/vendor/jquery-easing/jquery.easing.min.js"></script>

<!-- Custom JavaScript for this theme -->
<script src="~/Scripts/scrolling-nav.js"></script>


<script src="~/Scripts/ajax.js"></script>


<body id="page-top">

    <!-- Navigation -->
    <nav class="navbar navbar-expand-lg navbar-dark bg-dark fixed-top" id="mainNav">
        <div class="container">
            <a href="@Url.Action("Index", "Home")" class="navbar-brand js-scroll-trigger" href="#page-top">GeSoc</a>
            <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarResponsive" aria-controls="navbarResponsive" aria-expanded="false" aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
            </button>
            <div class="collapse navbar-collapse" id="navbarResponsive">
                <ul class="navbar-nav ml-auto">
                    <li class="nav-item">
                        <a href="@Url.Action("Index", "Home")#about" class="nav-link js-scroll-trigger">EGRESOS</a>
                    </li>
                    <li class="nav-item">
                        <a href="@Url.Action("Index", "Home")#services" class="nav-link js-scroll-trigger">INGRESOS</a>
                    </li>
                    <li class="nav-item">
                        <a href="@Url.Action("Index", "Home")#proyectos" class="nav-link js-scroll-trigger">PROYECTOS</a>
                    </li>
                    <li class="nav-item">
                        <a href="@Url.Action("Index", "Home")#proveedores" class="nav-link js-scroll-trigger">PROVEEDORES</a>
                    </li>
                    <li>
                        <a href="@Url.Action("Index", "Home")#criterios_y_categorias" class="nav-link js-scroll-trigger">CRITERIOS Y CATEGORIAS</a>
                    </li>
                    <li class="nav-item">
                        <a href="@Url.Action("Index", "Home")#bitacora_de_operaciones" class="nav-link js-scroll-trigger">BITACORA</a>
                    </li>

                    <li class="nav-item">
                        <a class="nav-link js-scroll-trigger" href="@Url.Action("Index", "Home")#contact">GitHub</a>
                    </li>
                    <li class="nav-item">
                        <a onclick="myFunction(0)" class="nav-link js-scroll-trigger">@Session["UserName"]</a>
                    </li>
                    <li id="0">
                        <a href="@Url.Action("Login", "Home")" class="nav-link js-scroll-trigger" style="display:block; color:firebrick">Cerrar sesion</a>
                    </li>
                </ul>
            </div>
        </div>
    </nav>


    <section id="services" class="bg-light">
        <div class="container">
            <div class="row">
                <div class="col-lg-8 mx-auto">

                    <h2>INGRESOS</h2>

                    <h6></h6>
                    <button onclick="myFunction(1)" target="_blank" class="button button2">Cargar nuevo ingreso</button>
                    <div id="1" style="display:none;">
                        <p></p>
                        @using (Html.BeginForm("CargarIngreso", "Ingreso", FormMethod.Post))
                        {
                            <p style="margin-left:40px;"><input type="text" name="descripcion" placeholder="Descripcion" class="button button3"></p>
                            <p style="margin-left:40px;"><input type="text" name="total" placeholder="Total" class="button button3"></p>

                            <p style="margin-left:40px;"><button id="cargarIngreso" class="button button4">Cargar ingreso</button></p>

                            <div id="ingresoInfo"></div>
                        }
                    </div>

                    <h6></h6>
                    <button onclick="myFunction(2)" target="_blank" class="button button2">Asociar ingresos y egresos</button>

                    <div id="2" style="display:none;">
                        <p>Seleccione el proceso de vinculacion</p>

                        <button onclick="myFunction(3)" target="_blank" class="button button2">Fecha primer egreso</button>
                        <div id="3" style="display:none;">
                            @using (Html.BeginForm("FechaPrimerEgreso", "Ingreso", FormMethod.Post))
                            {
                                <p>Ingrese los ID de los egresos separados por -</p>
                                <p style="margin-left:40px;"><input type="text" name="id_egresos" placeholder="ID egresos" class="button button3"></p>
                                <p>Ingrese los ID de los ingresos separados por -</p>
                                <p style="margin-left:40px;"><input type="text" name="id_ingresos" placeholder="ID ingresos" class="button button3"></p>
                                <button style="margin-left:40px;" id="fechaPrimerEgreso" class="button button4">Asociar</button>
                            }
                        </div>
                        <p></p>
                        <button onclick="myFunction(4)" target="_blank" class="button button2">Valor primer egreso</button>
                        <div id="4" style="display:none;">
                            @using (Html.BeginForm("ValorPrimerEgreso", "Ingreso", FormMethod.Post))
                            {
                                <p>Ingrese los ID de los egresos separados por -</p>
                                <p style="margin-left:40px;"><input type="text" name="id_egresos" placeholder="ID egresos" class="button button3"></p>
                                <p>Ingrese los ID de los ingresos separados por -</p>
                                <p style="margin-left:40px;"><input type="text" name="id_ingresos" placeholder="ID ingresos" class="button button3"></p>
                                <button style="margin-left:40px;" id="valorPrimerEgreso" class="button button4">Asociar</button>
                            }
                        </div>
                        <p></p>
                        <button onclick="myFunction(5)" target="_blank" class="button button2">Valor primer ingreso</button>
                        <div id="5" style="display:none;">

                            @using (Html.BeginForm("ValorPrimerIngreso", "Ingreso", FormMethod.Post))
                            {
                                <p>Ingrese los ID de los egresos separados por -</p>
                                <p style="margin-left:40px;"><input type="text" name="id_egresos" placeholder="ID egresos" class="button button3"></p>
                                <p>Ingrese los ID de los ingresos separados por -</p>
                                <p style="margin-left:40px;"><input type="text" name="id_ingresos" placeholder="ID ingresos" class="button button3"></p>
                                <button style="margin-left:40px;" id="valorPrimerIngreso" class="button button4">Asociar</button>
                            }
                        </div>

                    </div>

                    </div>
            </div>
        </div>
    </section>
</body>